{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "eslint:recommended", "plugin:jest/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "class-methods-use-this": "off"
  }
}
